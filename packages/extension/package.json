{
  "name": "extension",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "build:watch": "vite build --watch",
    "preview": "NODE_ENV=development npm run build && concurrently 'npm:build:watch' 'npm:_preview' 'npm:svelte-check:watch'",
    "_preview": "vite preview",
    "svelte-check": "svelte-check",
    "svelte-check:watch": "svelte-check --watch --preserveWatchOutput",
    "sb": "storybook dev --port 6006 --no-open",
    "build-storybook": "storybook build",
    "eslint": "eslint --ext .js,.ts src --fix",
    "eslint:fix": "npm run eslint -- --fix",
    "test": "vitest"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.2.2",
    "@storybook/addon-essentials": "^7.2.2",
    "@storybook/addon-interactions": "^7.2.2",
    "@storybook/addon-links": "^7.2.2",
    "@storybook/addon-svelte-csf": "^3.0.7",
    "@storybook/blocks": "^7.2.2",
    "@storybook/svelte": "^7.2.2",
    "@storybook/svelte-vite": "^7.2.2",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/theming": "^7.0.0",
    "@sveltejs/vite-plugin-svelte": "^2.4.2",
    "@tsconfig/svelte": "^5.0.0",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-svelte": "^2.32.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.2.2",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.6",
    "tslib": "^2.6.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.27.0"
  },
  "dependencies": {
    "@event-calendar/core": "^1.5.1",
    "@event-calendar/interaction": "^1.5.1",
    "@event-calendar/time-grid": "^1.5.1",
    "concurrently": "^8.2.0",
    "lodash": "^4.17.21"
  }
}